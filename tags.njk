---
title: Tags
layout: layout.njk
permalink: /tags/index.html
---

<h1>All Tags</h1>

{% for tag in collections.allTags %}
  <h2 id="{{ tag | slug }}">{{ tag }}</h2>
  <ul>
    {% for post in collections[tag] %}
      <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
    {% endfor %}
  </ul>
{% endfor %}
