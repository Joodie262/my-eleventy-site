{
    "name": "eleventy-retro-theme",
    "version": "1.0.0",
    "description": "A retro lavender/plum themed Eleventy starter inspired by Grav.",
    "main": "index.js",
    "scripts": {
        "start": "eleventy --serve",
        "build": "cross-env PATH_PREFIX=/ OUT_DIR=_site eleventy",
        "build:gh": "cross-env PATH_PREFIX=/my-eleventy-site/ OUT_DIR=_ghsite eleventy",
        "deploy": "npm run build:gh && gh-pages -d _ghsite"
    },
    "keywords": [
        "eleventy",
        "11ty",
        "retro",
        "theme",
        "personal-site"
    ],
    "author": "JudyWinsor",
    "license": "MIT",
    "devDependencies": {
        "@11ty/eleventy": "^3.1.2",
        "@11ty/eleventy-navigation": "^1.0.4",
        "cross-env": "^10.0.0",
        "gh-pages": "^6.3.0"
    },
    "dependencies": {
        "@11ty/eleventy-plugin-rss": "^2.0.4"
    }
}